{"ast":null,"code":"// import { toHandlers } from 'vue';\nexport default {\n  // 放置所有變數的位置\n  data() {\n    return {\n      numA: 0,\n      numB: 0,\n      result: 0,\n      html: `\n            <div>\n                我是html\n            </div>\n            `\n    };\n  },\n  // 放置所有function的位置\n  methods: {\n    add() {\n      // 先判斷numA numB不為空\n      if (this.numA.toString().trim === '' || this.numB.toString().trim === '') return;\n      this.result = this.numA + this.numB;\n    }\n  }\n};","map":{"version":3,"names":["data","numA","numB","result","html","methods","add","toString","trim"],"sources":["/Users/lingzhen/Documents/GitHub/vue3_test0102/src/views/CalcView.vue"],"sourcesContent":["<script>\n\n// import { toHandlers } from 'vue';\nexport default {\n    // 放置所有變數的位置\n    data() {\n        return {\n            numA: 0,\n            numB: 0,\n            result: 0,\n            html: `\n            <div>\n                我是html\n            </div>\n            `\n        }\n    },\n    // 放置所有function的位置\n    methods: {\n        add() {\n            // 先判斷numA numB不為空\n            if (this.numA.toString().trim === '' || this.numB.toString().trim === '') return;\n            this.result = this.numA + this.numB;\n        },\n    },\n}\n</script>\n\n<template>\n    <!-- v-if  -->\n    <!-- v-else-if  -->\n    <!-- v-else -->\n\n    <!-- v-show -->\n\n    <!-- v-for 記得要補key  -->\n    <!-- v-bind 省略的寫法「：」 -->\n    <!-- v-model 資料input與變數產生連結 -->\n    <!-- v-on  省略的寫法「＠」後面連接觸發的事件名稱 -->\n    <!-- v-html 最常用於所見即所得編輯器 -->\n    <div class=\"calc\">\n        計算機\n        <div v-html=\"html\"></div>\n    </div>\n    <div>\n        <label>\n            數字A:\n            <input type=\"number\" v-model=\"numA\" class=\"inputStyle\">\n        </label>\n        <label>\n            數字B:\n            <input type=\"number\" v-model=\"numB\" class=\"inputStyle\">\n        </label>\n    </div>\n\n    <div class=\"btns\">\n        <button class=\"btn\" type=\"button\" @click=\"add()\">+</button>\n        <button class=\"btn\">-</button>\n        <button class=\"btn\">x</button>\n        <button class=\"btn\">/</button>\n    </div>\n\n    <div class=\"output\">\n        輸出結果：{{ result }}\n    </div>\n</template>\n\n<!-- 目的是不要影響到其他區塊 -->\n<style lang=\"scss\" scoped>\n.calc {\n    //font-size: 42px;\n}\n\n.inputStyle {\n    @apply border border-gray-600 border-[1px];\n}\n\n.btns {\n    @apply flex gap-[30px] m-[20px];\n\n    justify-content: center;\n\n    .btn {\n        @apply border border-black border-[2px] hover:front;\n        padding: 5px 15px;\n    }\n\n}\n</style>"],"mappings":"AAEA;AACA,eAAe;EACX;EACAA,IAAIA,CAAA,EAAG;IACH,OAAO;MACHC,IAAI,EAAE,CAAC;MACPC,IAAI,EAAE,CAAC;MACPC,MAAM,EAAE,CAAC;MACTC,IAAI,EAAE;;;;;IAKV;EACJ,CAAC;EACD;EACAC,OAAO,EAAE;IACLC,GAAGA,CAAA,EAAG;MACF;MACA,IAAI,IAAI,CAACL,IAAI,CAACM,QAAQ,CAAC,CAAC,CAACC,IAAG,KAAM,EAAC,IAAK,IAAI,CAACN,IAAI,CAACK,QAAQ,CAAC,CAAC,CAACC,IAAG,KAAM,EAAE,EAAE;MAC1E,IAAI,CAACL,MAAK,GAAI,IAAI,CAACF,IAAG,GAAI,IAAI,CAACC,IAAI;IACvC;EACJ;AACJ"},"metadata":{},"sourceType":"module","externalDependencies":[]}